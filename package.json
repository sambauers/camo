{
  "name": "@sambauers/camo",
  "displayName": "Contentful Active Migration Organiser (CAMO)",
  "description": "Applies and provides status for migrations associated with the target Contentful space.",
  "version": "1.1.8",
  "repository": {
    "type": "git",
    "url": "git://github.com/sambauers/camo.git"
  },
  "homepage": "https://github.com/sambauers/camo",
  "author": {
    "name": "Sam Bauers",
    "email": "sam@wopr.com.au"
  },
  "license": "MIT",
  "engines": {
    "node": "v16.17.0"
  },
  "type": "module",
  "bin": {
    "camo": "bin/index.js"
  },
  "files": [
    "license.md",
    "bin/**/*",
    "src/**/*"
  ],
  "scripts": {
    "copy-release-files": "ts-node bin/copy-release-files.ts",
    "release": "pnpm build && pnpm copy-release-files && pnpm publish dist",
    "build": "pnpm lint && pnpm test && rm -rf dist && tsc",
    "test": "jest --coverage",
    "lint": "eslint .",
    "dev": "ts-node bin/index.ts"
  },
  "dependencies": {
    "commander": "9.4.0",
    "common-tags": "1.8.2",
    "contentful-management": "10.12.1",
    "contentful-migration": "4.9.4",
    "dotenv": "16.0.2",
    "inquirer": "9.1.1"
  },
  "devDependencies": {
    "@tsconfig/node16": "1.0.3",
    "@types/common-tags": "1.8.1",
    "@types/eslint": "8.4.6",
    "@types/inquirer": "9.0.1",
    "@types/jest": "29.0.0",
    "@types/node": "18.7.14",
    "@types/prettier": "2.7.0",
    "@typescript-eslint/eslint-plugin": "5.36.1",
    "@typescript-eslint/parser": "5.36.1",
    "eslint": "8.23.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.2.1",
    "jest": "29.0.2",
    "prettier": "2.7.1",
    "ts-jest": "28.0.8",
    "ts-node": "10.9.1",
    "typescript": "4.8.3"
  }
}
