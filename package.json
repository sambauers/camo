{
  "name": "@sambauers/camo",
  "displayName": "Contentful Active Migration Organiser (CAMO)",
  "description": "Applies and provides status for migrations associated with the target Contentful space.",
  "version": "2.0.0",
  "repository": {
    "type": "git",
    "url": "git://github.com/sambauers/camo.git"
  },
  "homepage": "https://github.com/sambauers/camo",
  "author": {
    "name": "Sam Bauers",
    "email": "sam@wopr.com.au"
  },
  "license": "MIT",
  "type": "module",
  "bin": {
    "camo": "bin/index.js"
  },
  "files": [
    "license.md",
    "bin/**/*",
    "src/**/*"
  ],
  "packageManager": "pnpm@8.6.5",
  "scripts": {
    "clean": "rm -rf dist",
    "lint": "eslint .",
    "test": "jest --coverage",
    "build": "pnpm clean && pnpm lint && pnpm test && tsc",
    "copy-release-files": "ts-node bin/copy-release-files.ts",
    "release": "pnpm build && pnpm copy-release-files && pnpm publish dist",
    "dev": "ts-node bin/index.ts",
    "regenerate-pnpm-lock": "rm -rf ./node_modules & rm pnpm-lock.yaml & pnpm install"
  },
  "dependencies": {
    "commander": "10.0.1",
    "common-tags": "1.8.2",
    "contentful-management": "10.35.3",
    "contentful-migration": "4.12.9",
    "dotenv": "16.0.3",
    "inquirer": "9.2.2"
  },
  "devDependencies": {
    "@types/common-tags": "1.8.1",
    "@types/eslint": "8.37.0",
    "@types/inquirer": "9.0.3",
    "@types/jest": "29.5.1",
    "@types/node": "18.16.8",
    "@types/prettier": "2.7.2",
    "@typescript-eslint/eslint-plugin": "5.59.5",
    "@typescript-eslint/parser": "5.59.5",
    "eslint": "8.40.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-prettier": "4.2.1",
    "jest": "29.5.0",
    "prettier": "2.8.8",
    "ts-jest": "29.1.0",
    "ts-node": "10.9.1",
    "typescript": "5.0.4"
  },
  "pnpm": {
    "updateConfig": {
      "ignoreDependencies": [
        "@types/node"
      ]
    }
  }
}
